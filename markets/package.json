{
  "name": "klimadao-markets",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-args": "graph create --node $npm_config_node klimadao-bonds",
    "deploy-args": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node klimadao-bonds",
    "deploy-version": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node --version-label $npm_config_label klimadao-bonds",
    "deploy-hosted": "graph deploy --product hosted-service $npm_config_path",
    "deploy": "graph deploy --product hosted-service klimadao/klimadao-markets",
    "create-local": "graph create --node http://127.0.0.1:8020 klima-markets",
    "remove-local": "graph remove --node http://127.0.0.1:8020 klima-markets",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020 --ipfs http://127.0.0.1:5001 klimadao-markets"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.29.0",
    "@graphprotocol/graph-ts": "0.26.0"
  }
}
