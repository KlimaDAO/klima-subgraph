{
    "name": "polygon-liquidity",
    "version": "0.0.1",
    "repository": "https://github.com/KlimaDAO/klima-subgraphs/polygon-liquidity",
    "license": "MIT",
    "scripts": {
        "codegen": "rm -rf ./generated && graph codegen",
        "build": "graph build",
        "test": "graph test",
        "create-local": "graph create --node http://127.0.0.1:8020 placeholder",
        "remove-local": "graph remove --node http://127.0.0.1:8020 placeholder",
        "watch-local": "graph deploy --watch --debug --node http://127.0.0.1:8020/ --ipfs http://127.0.0.1:5001 placeholder",
        "deploy-local": "graph deploy --node http://127.0.0.1:8020 --ipfs http://127.0.0.1:5001 placeholder",
        "create-args": "graph create --node $npm_config_node placeholder",
        "deploy-args": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node placeholder",
        "deploy-version": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node --version-label $npm_config_label placeholder",
        "deploy-staging": "graph deploy --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/ rplaceholder"
    },
    "devDependencies": {
        "@graphprotocol/graph-cli": "^0.38.0",
        "@graphprotocol/graph-ts": "^0.29.3",
        "matchstick-as": "^0.5.0",
        "prettier": "2.7.1"
    },
    "private": true
}
